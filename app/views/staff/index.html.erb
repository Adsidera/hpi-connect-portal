<div class="row">
  <div class="col-md-9">
    <h1><%= t("users.staff_headline")%></h1>
      <br><br>
      <ul class="list-group">
      <% @users.each do |staff| %>
        <li class="list-group-item">
          <div class="row">
            <div class="col-md-12">
              <%= link_to staff.firstname+" "+staff.lastname, staff_path(staff), :class => "h4" %>
            </div>
          </div>
        </li>
      <% end %>
      </ul>
      <%= will_paginate @users, renderer: BootstrapPagination::Rails%>
  </div>
  <div class="col-md-3">
  </div>

</div>