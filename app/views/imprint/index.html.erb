<h1><%=  t("imprint.headline") %></h1>
<br>
<div class="container">
	<p>
    	<h5><b><%=  t("imprint.hpi.headline") %></b></h5>
	    <%=  t("imprint.hpi.street") %>
	    <br>
	    <%=  t("imprint.hpi.city") %>
	    <br>
	    <%=  t("imprint.tel") %>: <%=  t("imprint.hpi.tel") %>
	    <br>
	    <%=  t("imprint.fax") %>: <%=  t("imprint.hpi.fax") %>
	    <br>
	    <%=  t("imprint.email") %>: <%=  t("imprint.hpi.email") %>
	    <br>
	    <%=  t("imprint.internet") %>: <%= link_to t("imprint.hpi.internet_title"), t("imprint.hpi.internet_link"),"data-no-turbolink" => true %>
	</p>
	<br>
	<p>
	    <h5><b><%=  t("imprint.manager.headline") %>: </b></h5>
		<%=  t("imprint.manager.person") %>
		<br>
		<br>
		<%=  t("imprint.reg_office") %>: <%=  t("imprint.manager.court") %>
		<br>
		<%=  t("imprint.reg_number") %>: <%=  t("imprint.manager.court_num") %>
	</p>
	<br>
	<p>
	    <h5><b><%=  t("imprint.content_resp.headline") %>: </b></h5>
	    <%=  t("imprint.content_resp.name") %>
	</p>
	<br>
	<p>
	    <h5><b><%=  t("imprint.editor.headline") %>: </b></h5>
	    <%=  t("imprint.editor.name") %>
	</p>
	<br>
	<p>
	    <h5><b><%=  t("imprint.design.headline") %>: </b></h5>
	    <%=  t("imprint.design.name") %>
	</p>
	<br>
	<p>
	    <h5><b><%=  t("imprint.text.headline") %>: </b></h5>
	    <%=  t("imprint.text.name") %>
	</p>
	<br>
	<p>
	    <h5><b><%=  t("imprint.info.headline") %>: </b></h5>
	    <%=  t("imprint.info.text") %>
	</p>
</div>
